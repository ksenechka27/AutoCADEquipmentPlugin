<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">

  <PropertyGroup>
    <OutputType>Library</OutputType>
    <TargetFramework>net48</TargetFramework>
    <UseWindowsForms>true</UseWindowsForms>
    <RootNamespace>AutoCADEquipmentPlugin</RootNamespace>
    <AssemblyName>AutoCADEquipmentPlugin</AssemblyName>
    <Platforms>x64</Platforms>
    <OutputPath>bin\PluginOutput\</OutputPath>
  </PropertyGroup>

  <ItemGroup>
    <Reference Include="AcCoreMgd">
      <HintPath>$(AutoCAD_ReferencePath)\AcCoreMgd.dll</HintPath>
    </Reference>
    <Reference Include="AcDbMgd">
      <HintPath>$(AutoCAD_ReferencePath)\AcDbMgd.dll</HintPath>
    </Reference>
    <Reference Include="AcMgd">
      <HintPath>$(AutoCAD_ReferencePath)\AcMgd.dll</HintPath>
    </Reference>
    <Reference Include="System.Windows.Forms" />
    <Reference Include="System.Drawing" />
  </ItemGroup>

</Project>
